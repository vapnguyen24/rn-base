{
  "name": "thingiq",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "dev:android": "KEYSFILE=env/dev.json npx expo run:android",
    "staging:android": "KEYSFILE=env/staging.json npx expo run:android",
    "prod:android": "KEYSFILE=env/production.json npx expo run:android",
    "dev:ios": "KEYSFILE=env/dev.json npx expo run:ios",
    "staging:ios": "KEYSFILE=env/staging.json npx expo run:ios",
    "prod:ios": "KEYSFILE=env/production.json npx expo run:ios",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.0.3",
    "@shopify/flash-list": "1.7.6",
    "@shopify/react-native-skia": "v2.0.0-next.4",
    "@shopify/restyle": "^2.4.2",
    "@tanstack/react-query": "^5.80.6",
    "axios": "^1.9.0",
    "expo": "^53.0.10",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.0",
    "expo-dev-launcher": "^5.0.17",
    "expo-font": "~13.3.1",
    "expo-image": "~2.2.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.7",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.8",
    "expo-web-browser": "~14.1.6",
    "formik": "^2.4.6",
    "i18next": "^25.2.1",
    "immer": "^10.1.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-freeze": "^1.0.4",
    "react-i18next": "^15.5.2",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-charts": "^1.4.61",
    "react-native-keys": "^0.7.11",
    "react-native-mmkv": "^3.2.0",
    "react-native-otp-entry": "^1.8.4",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-version-check-expo": "^3.5.0",
    "react-native-web": "^0.20.0",
    "yup": "^1.6.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "typescript": "~5.8.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "private": true
}
